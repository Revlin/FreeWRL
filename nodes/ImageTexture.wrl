# One red box
Transform {
	translation -2 0 0
	children [
		Shape {
			geometry Box { }
			appearance Appearance {
				texture ImageTexture {url "tex.png"}
			}
		}
	]
}

# Long, textured box
Transform {
	translation 2 0 0
	children [
		Shape {
			geometry Box { size 1 1 2 }
			appearance Appearance {
				texture ImageTexture {url "tex.jpg"}
			}
		}
	]
}
